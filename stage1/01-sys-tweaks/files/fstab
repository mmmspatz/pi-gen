proc            /proc           proc    defaults          0       0
BOOTDEV  /boot           vfat    defaults,ro          0       2
ROOTDEV  /               btrfs    defaults,compress=zstd,noatime  0       1
AUXDEV  /mnt/btrfs_aux_root btrfs   defaults,compress=zstd,noatime   0   0
AUXDEV  /var/lib/docker     btrfs   defaults,compress=zstd,noatime,subvol=@docker   0   0
